{
  "name": "gke-pod-network",
  "cniVersion": "0.3.1",
  "plugins": [
    {
      "type": "ptp",
      "mtu": 1460,
      "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "10.52.1.0/24"}]
        ],
        "routes": [
          {"dst": "0.0.0.0/0"}
        ]
      }
    },
    {
      "type": "portmap",
      "capabilities": {
        "portMappings": true
      }
    }, {
      "type": "cilium-cni",
      "enable-route-mtu": true,
      "dpv2-fast-start-namespaces": "default,kube-system"
    }
  ]
}
